        <class-name>com.oracle.coherence.concurrent.atomic.AtomicLong$Serializer</class-name>
      </serializer>
    </user-type>

    <user-type>
      <type-id>913</type-id>
      <class-name>java.util.concurrent.atomic.AtomicMarkableReference</class-name>
