    </user-type>

    <user-type>
      <type-id>911</type-id>
      <class-name>java.util.concurrent.atomic.AtomicInteger</class-name>
      <serializer>
        <class-name>com.oracle.coherence.concurrent.atomic.AtomicInteger$Serializer</class-name>
